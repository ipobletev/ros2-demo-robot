<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Sequence name="Behavior">
      <Action ID="InitSystemSystemVerification" />
      <Fallback name="Planning">
        <Sequence name="EV_PLANNING">
          <Action ID="ActionPlanning" />
          <Fallback name="EvaluatePlanning">
            <Sequence name="EV_EXEC_PLANNING">
              <Action ID="ExecutePlanning" />
              <Fallback name="CheckExecution">
                <Condition ID="EV_ISSUE_IN_EXEC" />
                <Condition ID="EV_COMMAND_STOP" />
              </Fallback>
            </Sequence>
            <Condition ID="EV_PLANNING_COMPLETE" />
          </Fallback>
        </Sequence>
        <Sequence name="EV_OP_DECISION">
          <Action ID="OperatorDecision" />
          <Fallback name="DecisionLoop">
            <Condition ID="EV_RESUME_PLANNING" />
            <Condition ID="END_PLANNING" />
          </Fallback>
        </Sequence>
      </Fallback>
      <Action ID="Perception" />
    </Sequence>
  </BehaviorTree>
</root>